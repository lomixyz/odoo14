            <!-- <script type="text/javascript" src="/netaddiction_warehouse/static/src/js/reso_fornitore.js"/> -->
        </xpath>
    </template>

    <template id="netaddiction_warehouse_files_inventory_reports" name="netaddiction_warehouse_assets_backend" inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/netaddiction_warehouse/static/src/js/inventory_reports.js"/>
        </xpath>
    </template>

    <template id="netaddiction_warehouse_assets_backend_manifest" name="netaddiction_warehouse_assets_backend_manifest" inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/netaddiction_warehouse/static/src/js/spara_pacchi.js"/>
        </xpath>
    </template>

</odoo>
