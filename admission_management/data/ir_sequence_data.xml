<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for admission Management -->
        <record id="seq_admission_request" model="ir.sequence">
            <field name="name">admission request</field>
            <field name="code">admission.request</field>
            <field name="prefix">RQ</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>


        <record id="seq_admission_contracts" model="ir.sequence">
            <field name="name">admission contracts</field>
            <field name="code">admission.contracts</field>
            <field name="prefix">CONTR</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>


         <record id="seq_rent_contracts" model="ir.sequence">
            <field name="name">rent workers management</field>
            <field name="code">rent.workers.management</field>
            <field name="prefix">RentCONTR</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>


        <record id="seq_rent_cancel" model="ir.sequence">
            <field name="name">rent cancel</field>
            <field name="code">rent.cancel</field>
            <field name="prefix">CancelRent</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
